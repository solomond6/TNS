@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
<script>
    $('.loader-wrapper').hide();

    $(document).on('click', '.btnInport', function(){
        $('.loader-wrapper').show();
    })
    $.ajax({
        type: "GET",
        url: "@Url.Action("GetListHeaders", "Lists")",
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        data: {'id': "1" },
        success: function (data) {
            $.each(data, function (index, item) {
                jQuery.grep(item, function(headers, i) {
                    $(".headers").html(headers.H1 +' | '+headers.H2 +' | '+headers.H3 +' | '+headers.H4 +' | '+headers.H5 +' | '+headers.H6 +' | '+headers.H7 +' | '+headers.H8 +' | '+headers.H9 +' | '+headers.H10 +' |'+headers.H11 +' | '+headers.H12 +' | '+headers.H13 +' | '+headers.H14 +' | '+headers.H15 +' | '+headers.H16 +' | '+headers.H17 +' | '+headers.H18 +' | '+headers.H19 +' | '+headers.H20);
                });
            });
        }
    });
</script>